{
  "name": "Web Helper Extension",
  "version": "0.0.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.svg",
      "48": "icons/icon16.svg",
      "128": "icons/icon16.svg"
    }
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "permissions": ["storage"],
  "description": "A minimal skeleton browser extension",
  "icons": {
    "16": "icons/icon16.svg",
    "48": "icons/icon16.svg",
    "128": "icons/icon16.svg"
  },
  "web_accessible_resources": [
    {
      "resources": ["*.js", "*.html", "icons/*"],
      "matches": ["<all_urls>"]
    }
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self';"
  }
}